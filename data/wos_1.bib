@article{ ISI:000222750600002,
Author = {Doloreux, D},
Title = {{Regional Innovation Systems in Canada: A comparative study}},
Journal = {{REGIONAL STUDIES}},
Year = {{2004}},
Volume = {{38}},
Number = {{5}},
Pages = {{481-494}},
Month = {{JUL}},
Abstract = {{The Regional Innovation System (RIS) has become a leading approach in
   explaining the innovation processes and patterns experienced by firms
   and industries at the regional level. This paper investigates the
   innovation activities of small and medium sized enterprises in different
   regions, assesses the extent of their involvement in systemic innovation
   with other organizations, and examines the nature of regional and more
   diffused forms of interaction in innovation activity. The discussion
   draws its empirical substance from the Ottawa and Beauce regions of
   Canada, regions characterized by different preconditions to innovation
   in terms of industrial production structure and institutional
   environments. The results indicate that the innovation activities of
   surveyed firms in the two regions converge into a similar pattern, as
   far as innovation practices, information sources and the role of
   geography are concerned. In addition, the significance of the region as
   a support to innovation is not confirmed; firms make use of regional,
   national and even global knowledge sources to sustain innovation.}},
