@article{ ISI:000292922700003,
Author = {Fritsch, Michael and Slavtchev, Viktor},
Title = {{Determinants of the Efficiency of Regional Innovation Systems}},
Journal = {{REGIONAL STUDIES}},
Year = {{2011}},
Volume = {{45}},
Number = {{7, SI}},
Pages = {{905-918}},
Abstract = {{FRITSCH M. and SLAVTCHEV V. Determinants of the efficiency of regional
   innovation systems, Regional Studies. This paper analyses differences in
   the efficiency of regional innovation systems. Alternative measures for
   the efficiency of regional innovation systems based on the concept of a
   knowledge production function are discussed. The empirical findings
   suggest that spillovers from within the private sector as well as from
   universities and other public research institutions have a positive
   effect on the efficiency of private sector research and development. It
   is particularly the intensity of interactions between private and public
   sector research and development that increases the efficiency. It is
   found that regions dominated by large establishments tend to be less
   efficient than regions with a lower average establishment size.}},
