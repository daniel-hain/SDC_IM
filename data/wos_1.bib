@article{ ISI:A1995QP52800002,
Author = {FREEMAN, C},
Title = {{THE NATIONAL SYSTEM OF INNOVATION IN HISTORICAL-PERSPECTIVE}},
Journal = {{CAMBRIDGE JOURNAL OF ECONOMICS}},
Year = {{1995}},
