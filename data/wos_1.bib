@article{ ISI:000269412800006,
Author = {Dolfsma, Wilfred and Leydesdorff, Loet},
Title = {{Lock-in and break-out from technological trajectories: Modeling and
   policy implications}},
Journal = {{TECHNOLOGICAL FORECASTING AND SOCIAL CHANGE}},
Year = {{2009}},
Volume = {{76}},
Number = {{7}},
Pages = {{932-941}},
Month = {{SEP}},
Abstract = {{The prevailing models explaining how technologies develop along a
   specific trajectory largely focus on the circumstances that lead to
   technological lock-in. We contribute substantially to this area of
   research by investigating the circumstances under which technological
   development may break-out of a trajectory. We argue that for this to
   happen, a third selection mechanism-beyond those of the market and of
   technology-needs to upset the lock-in. We model the interaction, or
   mutual shaping among three selection mechanisms, and thus this paper
   also allows for a better understanding of when a technology will lock-in
   into a trajectory, when a technology may break-out of a lock-in, and
   when competing technologies may co-exist in a balance. As a system is
   conceptualized to gain a (third) degree of freedom, the possibility of
   bifurcation is introduced into the model. The equations, in which
   interactions between competition and selection mechanisms can be
   modeled, allow one to specify conditions for lock-in, competitive
   balance, and break-out. (C) 2009 Elsevier Inc. All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE INC}},
Address = {{360 PARK AVE SOUTH, NEW YORK, NY 10010-1710 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Dolfsma, W (Reprint Author), Univ Groningen, Sch Business \& Econ, POB 800, NL-9700 AV Groningen, Netherlands.
   Dolfsma, Wilfred, Univ Groningen, Sch Business \& Econ, NL-9700 AV Groningen, Netherlands.
   Leydesdorff, Loet, Univ Amsterdam, Amsterdam Sch Commun Res ASCoR, NL-1012 CX Amsterdam, Netherlands.}},
