@article{ ISI:000294359800001,
Author = {Mas-Verdu, Francisco and Wensley, Anthony and Alba, Martin and Garcia
   Alvarez-Coque, Jose Maria},
Title = {{How much does KIBS contribute to the generation and diffusion of
   innovation?}},
