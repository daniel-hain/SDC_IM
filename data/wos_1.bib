@article{ ISI:000313314900017,
Author = {Zhang, Jing and Liang, Xiong-jian},
Title = {{Promoting green ICT in China: A framework based on innovation system
   approaches}},
Journal = {{TELECOMMUNICATIONS POLICY}},
