@article{ ISI:000174811300011,
Author = {Teubal, M},
Title = {{What is the systems perspective to Innovation and Technology Policy(ITP)
   and how can we apply it to developing and newly industrialized
   economies?}},
Journal = {{JOURNAL OF EVOLUTIONARY ECONOMICS}},
Year = {{2002}},
Volume = {{12}},
Number = {{1-2}},
Pages = {{233-257}},
Month = {{MAR}},
