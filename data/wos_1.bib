@article{ ISI:000257336500005,
Author = {Lazaric, Nathalie and Longhi, Christian and Thomas, Catherine},
Title = {{Gatekeepers of knowledge versus platforms of knowledge: From potential
   to realized absorptive capacity}},
Journal = {{REGIONAL STUDIES}},
Year = {{2008}},
Volume = {{42}},
Number = {{6}},
Pages = {{837-852}},
Abstract = {{The development of clusters rests on geographical proximity, cognitive
   interactions as well as entrepreneurial initiatives. Sophia Antipolis, a
   multi-technology cluster in Valbonne, France, is a good illustration of
   the type of challenges local systems of innovation face in creating
   positive knowledge externalities. This paper shows that if the existence
   of `gatekeepers of knowledge' can generate the potential implementation
   of `absorptive capacity', its effective realization requires some
   additional effort regarding the transfer of knowledge into the cluster.
   The concept of `platform of knowledge' defined shows how a project of
   knowledge codification could generate externalities by creating new
   opportunities for effectively combining and absorbing knowledge.}},
