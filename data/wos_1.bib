@article{ ISI:000260021600001,
Author = {Fagerberg, Jan and Srholec, Martin},
Title = {{National innovation systems, capabilities and economic development}},
Journal = {{RESEARCH POLICY}},
Year = {{2008}},
Volume = {{37}},
Number = {{9}},
Pages = {{1417-1435}},
Month = {{OCT}},
Note = {{4th GLOBELICS 2006 Conference, Trivandrum, INDIA, OCT 04-06, 2006}},
Abstract = {{This paper focuses on the role of capabilities in economic development.
   In recent years, the quality and availability of data on different
   aspects of development have improved, and this provides new
   opportunities for investigating the reasons behind the large differences
   in economic development. Using factor analysis on data for 25 indicators
   and 115 countries between 1992 and 2004, we identify four different
   types of ``capabilities{''}: the development of the ``innovation
   system{''}, the quality of ``governance{''}, the character of the
   ``political system{''} and the degree of ``openness{''} of the economy.
   Innovation systems and governance are shown to be of particular
   importance for economic development. (C) 2008 Elsevier B.V. All rights
   reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
