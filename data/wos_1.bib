@article{ ISI:000298909700019,
Author = {Li, Xibao},
Title = {{Behind the recent surge of Chinese patenting: An institutional view}},
Journal = {{RESEARCH POLICY}},
Year = {{2012}},
Volume = {{41}},
Number = {{1}},
Pages = {{236-249}},
Month = {{FEB}},
Abstract = {{This paper examines a number of forces that have possibly contributed to
   the explosive growth of Chinese patenting over the past decade. After a
   review of previous hypotheses and conventional wisdom, this study
   proposes an additional explanation and argues that patent subsidy
   programs implemented by each provincial region have played an important
   role in the growth of Chinese patenting. This institutional change,
   taking place at the province-level, has induced an increase in patent
   propensity among not only firms, universities, and research institutes,
   but also individuals. Empirical evidence based on publicly available
   data provides solid support for this argument. It was also found that a
   larger fraction of applications are granted patent rights since the
   implementation of such programs, suggesting that reduction in patent
   application quality may not be a serious concern, unless the criteria
   used for patent examination have been lowered. (C) 2011 Elsevier B.V.
   All rights reserved.}},
