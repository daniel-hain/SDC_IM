@article{ ISI:000274142400003,
Author = {Lazzeretti, Luciana and Capone, Francesco and Cinti, Tommaso},
Title = {{The Regional Development Platform and ``Related Variety{''}: Some
   Evidence from Art and Food in Tuscany}},
Journal = {{EUROPEAN PLANNING STUDIES}},
Year = {{2010}},
Volume = {{18}},
Number = {{1, SI}},
Pages = {{27-45}},
Month = {{JAN}},
Abstract = {{A recent contribution to the regional/local development is the regional
   development platform (RDP), a toot of local policy and governance meant
   for the planning and implementation of a regional innovation system
   (RIS) with a sustainable and long-lasting competitive advantage. The aim
   of this pal)er is to contribute to the debate oil local economic
   development through platforms of regional development, offering some
   specific cases in which the RDP model is developed not only as a
   top-down policy tool ill support of innovation, but also as a bottom-up
   governance toot for the relationships among cognitively related
   industries. We introduce the case of all art and food platform in
   Maremma (south of Tuscany, Italy), where the related-variety approach is
   mainly focused oil cross-fertilization among related and unrelated
   resources and sectors and is specifically applied to a rural area.}},
