@article{ ISI:000255956300003,
Author = {Doloreux, David and Dionne, Steve},
Title = {{Is regional innovation system development possible in peripheral
   regions? Some evidence from the case of La Pocatiere, Canada}},
Journal = {{ENTREPRENEURSHIP AND REGIONAL DEVELOPMENT}},
Year = {{2008}},
Volume = {{20}},
Number = {{3}},
Pages = {{259-283}},
Month = {{MAY}},
Note = {{Meeting of the Association-de-Sciences-Regionales-de-Langue-Francaise,
   Grenoble, FRANCE, JUL 11-13, 2007}},
Organization = {{Assoc Sci Reg Langue Francaise}},
Abstract = {{The aim of this paper is to contribute to a greater understanding of the
   research on innovation systems in peripheral regions by providing a
   detailed account of the case of the La Pocatiere region in Canada. In
   analysing this case, we raise the following two questions: (1) what are
   the actors and structure of the innovation system in La Pocatiere?; (2)
   what are the key factors and dynamics leading to innovation activity, as
   well as to the transformation and growth of this regional innovation
   system? The empirical bases for the analyses are derived from various
   sources: historical documents, statistical data, and in-depth interviews
   with key individuals in private and public organizations.}},
