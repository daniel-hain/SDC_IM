@article{ ISI:000179836000016,
Author = {Intarakumnerd, P and Chairatana, PA and Tangchitpiboon, T},
Title = {{National innovation system in less successful developing countries: the
   case of Thailand}},
Journal = {{RESEARCH POLICY}},
Year = {{2002}},
Volume = {{31}},
Number = {{8-9}},
Pages = {{1445-1457}},
Month = {{DEC}},
Note = {{Conference of the Danish-Research-Unit-for-Industrial-Dynamics, AALBORG,
   DENMARK, JUN 12-15, 2001}},
Organization = {{Danish Res Unit Ind Dynam}},
Abstract = {{This paper, using Thailand as a case study, aims at understanding the
   national innovation system (NIS) in developing countries which are less
   successful in technological catching-up. In contrast to developed
   countries, the development level of Thailand's NIS does not link to its
   economic structural development level. As Thailand moves from
   agricultural to an increasingly industrial economy, its NIS remains weak
   and fragmented. The mismatch between the two affected Thailand's
   competitiveness and partially contributed to the recent economic crisis.
   Studies of NIS in countries like Thailand should focus on factors
   contributing to the long-running perpetuation of weak and fragmented
   NIS. (C) 2002 Elsevier Science B.V. All rights reserved.}},
