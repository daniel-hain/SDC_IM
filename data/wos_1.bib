@article{ ISI:000270344000001,
Author = {Surrs, Roald A. A. and Hekkert, Marko P.},
Title = {{Cumulative causation in the formation of a technological innovation
   system: The case of biofuels in the Netherlands}},
Journal = {{TECHNOLOGICAL FORECASTING AND SOCIAL CHANGE}},
Year = {{2009}},
Volume = {{76}},
Number = {{8}},
Pages = {{1003-1020}},
Month = {{OCT}},
Abstract = {{Despite its worldwide success, the innovation systems approach is often
   criticised for being theoretically underdeveloped. This paper aims to
   contribute to the conceptual and methodological basis of the
   (technological) innovation systems approach. We propose an alteration
   that improves the analysis of dynamics, especially with respect to
   emerging innovation systems. We do this by expanding on the
   technological innovation systems and system functions literature, and by
   employing the method of `event history analysis'. By mapping events, the
   interactions between system functions and their development over time
   can be analysed. Based on this it becomes possible to identify forms of
   positive feedback, i.e. cumulative causation. As an illustration of the
   approach, we assess the biofuels innovation system in The Netherlands as
   it evolved from 1990 to 2007. (C) 2009 Elsevier Inc. All rights
   reserved.}},
